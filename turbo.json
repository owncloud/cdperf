{
	"$schema": "https://turborepo.org/schema.json",
	"pipeline": {
		"lint": {
      "outputMode": "new-only"
    },
		"lint:fix": {
      "outputMode": "new-only"
    },
		"build": {
      "outputMode": "new-only",
      "dependsOn": ["^build"],
			"outputs": [
				"dist/**",
				"artifacts/**"
			]
		},
    "dev": {
      "cache": false,
      "persistent": true
    },
    "clean": {
      "cache": false
    }
	}
}
